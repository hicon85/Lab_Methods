using System;

namespace _07.Repeat_String
{
    class Program
    {
        static void Main(string[] args)
        {
            string name = Console.ReadLine();
            int n = int.Parse(Console.ReadLine());

            string result = GoodJob(name, n);

            Console.WriteLine(result);
        }
        private static string GoodJob(string name, int n)
        {
            string total = "";

            for (int i = 0; i < n; i++)
            {
                total += name;
            }
            return total;
        }
    }
}
