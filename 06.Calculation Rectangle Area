using System;

namespace _06.Calculation_Rectangle_Area
{
    class Program
    {
        static void Main(string[] args)
        {
            int height = int.Parse(Console.ReadLine());
            int widht = int.Parse(Console.ReadLine());

            int area = GoodJob(height, widht);

            Console.WriteLine(area);
        }
        private static int GoodJob(int height, int width)
        {
            return height * width;
        }
    }
}
